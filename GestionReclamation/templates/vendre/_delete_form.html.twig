<form method="post" action="{{ path('app_vendre_delete', {'idequipement': vendre.idequipement}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vendre.idequipement) }}">
    <button class="btn">Delete</button>
</form>
